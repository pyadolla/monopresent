        "localhost:3000/Users/yadollap/sandbox/monopresent/packages/example-presentation/src/*": "${workspaceFolder}/packages/example-presentation/src/*",
        "localhost:3000/Users/yadollap/sandbox/monopresent/packages/immersion-presentation/src/*": "${workspaceFolder}/packages/immersion-presentation/src/*"
      }
      // "sourceMapPathOverrides": {
